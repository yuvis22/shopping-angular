{
  "name": "shopping-app-server",
  "version": "1.0.0",
  "description": "Backend server for shopping app",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "set-admin": "tsx src/scripts/set-admin.ts",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^5.0.0",
    "backblaze-b2": "^1.7.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
